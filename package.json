    "migrate:dry": "ts-node src/tools/db-migrate/cli.ts migrate --dry-run",
    "migrate:status": "ts-node src/tools/db-migrate/cli.ts status",
    "migrate:create": "ts-node src/tools/db-migrate/cli.ts create",
    "schema:diff": "ts-node src/tools/db-migrate/cli.ts schema:diff",
    "schema:sync": "ts-node src/tools/db-migrate/cli.ts schema:sync"
